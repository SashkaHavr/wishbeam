{
  "name": "@wishbeam/prettier-config",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./prettier.config.js"
  },
  "scripts": {
    "clean": "bun exec \"rm -rf node_modules .turbo\"",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "@wishbeam/typescript-config": "workspace:*",
    "typescript": "catalog:"
  }
}
