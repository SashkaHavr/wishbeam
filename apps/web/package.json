{
  "name": "@wishbeam/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "git clean -xdf .turbo node_modules dist .tanstack .nitro .output",
    "component": "pnpm dlx shadcn@latest add",
    "postcomponent": "pnpm run format",
    "dev": "vite",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "bun exec \"NODE_ENV=production bun run .output/server/index.mjs\"",
    "typecheck": "tsc --noEmit"
